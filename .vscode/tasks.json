{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "echo",
            "type": "shell",
            "command": "make all"
        },
        {
            "type": "cppbuild",
            "label": "C/C++: arm-none-eabi-g++ build active file",
            "command": "/usr/local/bin/arm-none-eabi-g++",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "-fno-exceptions",
                "-fno-rtti",
                "-std=gnu++14",
                "-std=gnu11",
                "-Wall",
                "-Wno-attributes",
                "-Wno-strict-aliasing",
                "-Wno-maybe-uninitialized",
                "-mcpu=cortex-m7",
                "-mfpu=fpv5-d16",
                "-mfloat-abi=hard",
                "-mthumb"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build",
            "detail": "Task generated by Debugger."
        }
    ]
}